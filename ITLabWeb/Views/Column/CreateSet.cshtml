<form method="post" action="~/Column/CreateSet">
    <p>
        <label>Назва</label><br />
        @Html.TextBox("name")
    </p>
    <p>
        <label>Значення:</label><br />
        @Html.TextBox("values")
    </p>
    <p>
        <input hidden name="tabName" value="@ViewBag.tabName" />
    </p>
    <p>
        <input type="submit" value="Створити" />
    </p>
</form>
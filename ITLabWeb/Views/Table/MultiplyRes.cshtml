@using ITLab.Classes.Database
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    ViewData["Title"] = "Table";
}

<h3>@ViewBag.errMsg</h3>
<table>
    <tr>
        @foreach (Column col in ViewBag.Table.Columns)
        {
            <th>@col.Name</th>
        }
    </tr>
    @{
        int i = 0;
    }
    @foreach (Row row in ViewBag.Table.Rows)
    {

        <tr>

            @foreach (string val in row.Values)
            {
                <td>@val</td>
            }
        </tr>
    }
</table>